{
  "name": "vimath",
  "version": "0.0.10",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "npx jest --verbose --config jest.config.js",
    "test-specs": "jasmine-browser-runner runSpecs",
    "bundle-prod-ts": "./node_modules/.bin/esbuild src/index.ts --bundle --sourcemap --outdir=./dist/out --global-name=${npm_package_name} --format=esm",
    "bundle-dev": "./node_modules/.bin/esbuild src/index.ts --bundle --sourcemap --global-name=${npm_package_name} --outfile=./dist/out/${npm_package_name}.dev.js --format=esm",
    "bundle-prod": "./node_modules/.bin/esbuild src/index.ts --bundle --minify --global-name=${npm_package_name} --outfile=./dist/out/${npm_package_name}.production.js --format=esm",
    "bundle-local": "./node_modules/.bin/esbuild src/index.ts --bundle --sourcemap --outdir=./dist/out --global-name=${npm_package_name} --watch --format=esm",
    "local-publish": "rm -rf dist && npm run build && npm run bundle-prod && cp package.json dist && cd dist && npm pack --pack-destination .",
    "publish-to-npm": "rm -rf dist && npm run bundle-dev && npm run bundle-prod && cp package.json dist && npm version patch && npm publish --access public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@blparker/canvas-test": "^1.0.15",
    "@jest/globals": "^29.6.4",
    "@types/jasmine": "^4.3.5",
    "@types/jest": "^29.5.4",
    "@types/jsdom": "^21.1.3",
    "@types/ungap__structured-clone": "^0.3.0",
    "@ungap/structured-clone": "^1.2.0",
    "@web/test-runner": "^0.17.1",
    "canvas": "^2.11.2",
    "esbuild": "^0.19.5",
    "esbuild-plugin-tsc": "^0.4.0",
    "jasmine": "^5.1.0",
    "jasmine-browser-runner": "^2.2.0",
    "jasmine-core": "^5.1.1",
    "jest": "^29.6.4",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.4",
    "jest-puppeteer": "^9.0.0",
    "jsdom": "^22.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.9",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/offscreencanvas": "^2019.7.1",
    "decimal.js": "^10.4.3",
    "mathjax-full": "^3.2.2"
  },
  "type": "module"
}
